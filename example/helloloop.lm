use "std" -> std;

act[io.out] main() -> i32 {
  var i32 i = 0;
  while (i < 10) {
    std.println("Hello, while loop!");
    i++;
  }
  for (var i32 i = 0; i < 10; i++) {
    std.println("Hello, for loop!");
  }
  for (val a : 0 .. 9) {
    std.println("Hello, range-based for loop!");
  }
  return 0;
}
